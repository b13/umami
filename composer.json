{
	"name": "b13/umami",
	"type": "typo3-cms-extension",
	"description": "Umami Tracking for your TYPO3 site",
	"license": ["GPL-2.0-or-later"],
	"require": {
		"typo3/cms-backend": "^11 || ^12"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "umami"
		}
	},
	"autoload": {
		"psr-4": {
			"B13\\Umami\\": "Classes"
		}
	},
	"config": {
		"sort-packages": true
	},
	"require-dev": {
		"typo3/coding-standards": "^0.5.5"
	}
}
